<!--components/input_number/com_input_number.wxml-->

<view class="weui-cell weui-cell_input weui-cell_vcode">
    <view class="weui-cell__hd">
        <view class="weui-label">{{template.title}}</view>
    </view>
    <view class="weui-cell__bd">
        <input class="weui-input" type='number' value='{{value}}' disabled='{{disabled}}' bindinput='onInputChanged' placeholder="请输入{{template.title}}" />
    </view>
</view>

